{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ctlestablecimiento</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ctlEstablecimiento.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ ctlEstablecimiento.nombre }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ ctlEstablecimiento.direccion }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ ctlEstablecimiento.telefono }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ ctlEstablecimiento.fax }}</td>
            </tr>
            <tr>
                <th>Latitud</th>
                <td>{{ ctlEstablecimiento.latitud }}</td>
            </tr>
            <tr>
                <th>Longitud</th>
                <td>{{ ctlEstablecimiento.longitud }}</td>
            </tr>
            <tr>
                <th>Anioapertura</th>
                <td>{{ ctlEstablecimiento.anioApertura }}</td>
            </tr>
            <tr>
                <th>Codestabconsumos</th>
                <td>{{ ctlEstablecimiento.codestabConsumos }}</td>
            </tr>
            <tr>
                <th>Codestabsiaf</th>
                <td>{{ ctlEstablecimiento.codestabSiaf }}</td>
            </tr>
            <tr>
                <th>Coducsf</th>
                <td>{{ ctlEstablecimiento.codUcsf }}</td>
            </tr>
            <tr>
                <th>Instmortalidad</th>
                <td>{{ ctlEstablecimiento.instMortalidad }}</td>
            </tr>
            <tr>
                <th>Digitamortalidad</th>
                <td>{{ ctlEstablecimiento.digitaMortalidad }}</td>
            </tr>
            <tr>
                <th>Cvcd4</th>
                <td>{{ ctlEstablecimiento.cvcd4 }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{% if ctlEstablecimiento.activo %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Idusuarioreg</th>
                <td>{{ ctlEstablecimiento.idUsuarioReg }}</td>
            </tr>
            <tr>
                <th>Fechahorareg</th>
                <td>{% if ctlEstablecimiento.fechaHoraReg %}{{ ctlEstablecimiento.fechaHoraReg|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Idusuariomod</th>
                <td>{{ ctlEstablecimiento.idUsuarioMod }}</td>
            </tr>
            <tr>
                <th>Fechahoramod</th>
                <td>{% if ctlEstablecimiento.fechaHoraMod %}{{ ctlEstablecimiento.fechaHoraMod|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Idestablesimow</th>
                <td>{{ ctlEstablecimiento.idestablesimow }}</td>
            </tr>
            <tr>
                <th>Idestablevigepes</th>
                <td>{{ ctlEstablecimiento.idestablevigepes }}</td>
            </tr>
            <tr>
                <th>Estadovigepes</th>
                <td>{{ ctlEstablecimiento.estadoVigepes }}</td>
            </tr>
            <tr>
                <th>Estadovacunas</th>
                <td>{{ ctlEstablecimiento.estadoVacunas }}</td>
            </tr>
            <tr>
                <th>Estadoseps</th>
                <td>{{ ctlEstablecimiento.estadoSeps }}</td>
            </tr>
            <tr>
                <th>Estadosimow</th>
                <td>{{ ctlEstablecimiento.estadoSimow }}</td>
            </tr>
            <tr>
                <th>Idestablesumeve</th>
                <td>{{ ctlEstablecimiento.idestablesumeve }}</td>
            </tr>
            <tr>
                <th>Estadosumeve</th>
                <td>{{ ctlEstablecimiento.estadoSumeve }}</td>
            </tr>
            <tr>
                <th>Cabezamicrored</th>
                <td>{% if ctlEstablecimiento.cabezaMicrored %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Hospitalizacion</th>
                <td>{% if ctlEstablecimiento.hospitalizacion %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Iduser</th>
                <td>{{ ctlEstablecimiento.idUser }}</td>
            </tr>
            <tr>
                <th>Ipuser</th>
                <td>{{ ctlEstablecimiento.ipUser }}</td>
            </tr>
            <tr>
                <th>Detalleschema</th>
                <td>{{ ctlEstablecimiento.detalleSchema }}</td>
            </tr>
            <tr>
                <th>Enableschema</th>
                <td>{% if ctlEstablecimiento.enableSchema %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Estadoschema</th>
                <td>{{ ctlEstablecimiento.estadoSchema }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('establecimiento_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('establecimiento_edit', { 'id': ctlEstablecimiento.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
