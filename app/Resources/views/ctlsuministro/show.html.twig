{% extends 'base.html.twig' %}

{% block body %}
    <h1>Ctlsuministro</h1>

    <table>
        <tbody>
            <tr>
                <th>Nombresuministro</th>
                <td>{{ ctlSuministro.nombreSuministro }}</td>
            </tr>
            <tr>
                <th>Detallesuministro</th>
                <td>{{ ctlSuministro.detalleSuministro }}</td>
            </tr>
            <tr>
                <th>Registroschema</th>
                <td>{% if ctlSuministro.registroSchema %}{{ ctlSuministro.registroSchema|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Detalleschema</th>
                <td>{{ ctlSuministro.detalleSchema }}</td>
            </tr>
            <tr>
                <th>Useridschema</th>
                <td>{{ ctlSuministro.userIdSchema }}</td>
            </tr>
            <tr>
                <th>Ipuserschema</th>
                <td>{{ ctlSuministro.ipUserSchema }}</td>
            </tr>
            <tr>
                <th>Estadoschema</th>
                <td>{{ ctlSuministro.estadoSchema }}</td>
            </tr>
            <tr>
                <th>Enableschema</th>
                <td>{{ ctlSuministro.enableSchema }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('suministro_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('suministro_edit', { 'id': ctlSuministro.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
