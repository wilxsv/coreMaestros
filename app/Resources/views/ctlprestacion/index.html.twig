{% extends 'base.html.twig' %}
{% block title %}Listado de Prestación{% endblock %}
{% block body %}
    <table id="sample-table" class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <th>Nombreprestacion</th>
                <th>Operaciones</th>
            </tr>
        </thead>
        <tbody>
        {% for ctlPrestacion in ctlPrestacions %}
            <tr>
                <td>{{ ctlPrestacion.nombrePrestacion }}</td>
                <td>
                  <a href="{{ path('prestacion_show', { 'id': ctlPrestacion.id }) }}">Ver <i class="icon-show bigger-120"></i></a>
                  <a href="{{ path('prestacion_edit', { 'id': ctlPrestacion.id }) }}">Editar <i class="icon-edit bigger-120"></i></a>
                </td
            </tr>
        {% endfor %}
        </tbody>
    </table>
<a href="{{ path('prestacion_new') }}" class="btn btn-primary">Agregar Prestación</a>
{% endblock %}
{% block footer_js %}<script type="text/javascript">$(function(){ var oTable2 = $('#sample-table').dataTable(); })</script>{% endblock %}
